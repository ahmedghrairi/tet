<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Dossier.
    -->
    <changeSet id="20191010150423-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="dossier_id"
                                 baseTableName="dossier_client"
                                 constraintName="fk_dossier_client_dossier_id"
                                 referencedColumnNames="id"
                                 referencedTableName="dossier"/>

        <addForeignKeyConstraint baseColumnNames="client_id"
                                 baseTableName="dossier_client"
                                 constraintName="fk_dossier_client_client_id"
                                 referencedColumnNames="id"
                                 referencedTableName="client"/>
        
    </changeSet>
</databaseChangeLog>
